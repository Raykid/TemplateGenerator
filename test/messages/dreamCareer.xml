<config>
    <message>
        <default method="GET" response="response->$a-{name}" protocol="http" comment="" domainindex="0"
                 contentType="application/x-www-form-urlencoded"/>
        <extra>
            <item name="AchieveNewCareer" method="POST"
                  url="/studentMobile/growingworld/dreamcareer/achievenewcareer.vpage" comment="获得新职业">
                <field name="careerId" type="string" comment="职业Id"/>
            </item>
            <item name="ChooseMajorCareer" method="POST"
                  url="/studentMobile/growingworld/dreamcareer/choosemajorcareer.vpage" comment="选择切换职业">
                <field name="careerId" type="string" comment="职业Id"/>
            </item>
            <item name="UpgradeCareer" method="POST" url="/studentMobile/growingworld/dreamcareer/upgradecareer.vpage"
                  comment="升级职业">
                <field name="careerId" type="string" comment="职业Id"/>
            </item>
            <item name="CompletePromotionExam" method="POST"
                  url="/studentMobile/growingworld/dreamcareer/completepromotionexam.vpage" comment="完成晋级考试">
                <field name="careerId" type="string" comment="职业Id"/>
            </item>
            <item name="CareerWorkIndex" method="POST"
                  url="/studentMobile/growingworld/dreamcareer/careerworkindex.vpage" comment="工作首页">
                <field name="careerId" type="string" comment="职业Id"/>
            </item>
            <item name="DoWork" method="POST" url="/studentMobile/growingworld/dreamcareer/dowork.vpage" comment="完成工作">
                <field name="completeTimes" type="int" comment="完成次数"/>
                <field name="careerWorkId" type="string" comment="工作id"/>
            </item>
            <item name="CommunityIndex" method="POST" url="/studentMobile/growingworld/dreamcareer/communityindex.vpage"
                  comment="社区首页"/>
            <item name="DreamCareerIndex" method="POST" url="/studentMobile/growingworld/dreamcareer/index.vpage"
                  comment="梦想职业首页"/>
        </extra>
    </message>
    <response>
        <default>
			<field name="success" type="boolean" comment="返回值时表示是否成功"/>
			<field name="errorCode" type="int" comment="success是false时表示错误码"/>
			<field name="info" type="string" comment="success是false时表示错误信息"/>
			<field name="currentTime" type="long" comment="当前的服务器时间戳（毫秒）"/>
		</default>
        <extra>
            <item name="AchieveNewCareer" comment="获得新职业"/>
            <item name="ChooseMajorCareer" comment="选择切换职业"/>
            <item name="UpgradeCareer" comment="升级职业"/>
            <item name="CompletePromotionExam" comment="完成晋级考试"/>
            <item name="CareerWorkIndex" comment="工作首页">
                <field name="workPropId" type="string" comment="工作证道具id"/>
                <field name="workPropNum" type="int" comment="工作证数量"/>
                <field name="completeWorkVOList" type="CompleteWorkVO[]" comment="完成工作次数"/>
            </item>
            <item name="DoWork" comment="完成工作"/>
            <item name="CommunityIndex" comment="社区首页">
                <field name="communityInfo" type="ClazzCommunityVO" comment="城市信息"/>
                <field name="goalInfo" type="ClazzGoalVO" comment="城市目标信息"/>
                <field name="mayer" type="string" comment="市长名字，空字符串如果没有市长"/>
                <field name="buildings" type="OfficeBuildingVO[]" comment="建筑物列表"/>
                <field name="cgPopup" type="ClazzGoalPopupVO" comment="城市目标弹窗，如果为null不弹"/>
                <field name="obuPopup" type="OfficeBuildingUpgradedPopupVO[]" comment="建筑物升级提醒弹窗，如果为null或者列表为空不弹"/>
                <field name="ccuPopup" type="ClazzCommunityUpgradedPopupVO" comment="城市升级提醒弹窗，如果为null不弹"/>
                <field name="srPopup" type="SuperiorRewardPopupVO" comment="上级奖励弹窗，如果为null不弹"/>
            </item>
            <item name="DreamCareerIndex" comment="梦想职业首页">
                <field name="userCareer" type="UserCareerVO" comment="用户当前职业信息,为空表示不存在"/>
                <field name="haveBox" type="boolean" comment="是否有宝箱"/>
                <field name="upgrade" type="boolean" comment="是否有可以升级"/>
                <field name="achieveCareerList" type="UserCareerVO[]" comment="用户拥有职业列表"/>
                <field name="weekExp" type="int" comment="本周经验值"/>
                <field name="weekBuildingExp" type="int" comment="本周建设值"/>
                <field name="fairyType" type="string" comment="用户精灵性别"/>
            </item>
        </extra>
    </response>
</config>